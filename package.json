{
  "name": "link-file-plugin",
  "version": "0.0.7",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "install-peers",
    "build": "rollup -c",
    "test": "yarn run build && webpack --config ./example/webpack.config.js",
    "release": "yarn run build && git add . && git commit  && npm version patch && npm publish"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "2string-loader": "^1.0.0",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "execa": "^4.0.3",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prettier-plugin-jsdoc": "^0.0.23",
    "rollup": "^2.23.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "loader-utils": "^2.0.0",
    "webpack": "^4.44.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "eslintIgnore": [
    "**/dist/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "install-peers-cli": "^2.2.0"
  }
}
